{
  "name": "@untappd/components",
  "version": "1.0.0-beta.0",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --mode development",
    "build": "rollup -c",
    "build:docs": "webpack --config=webpack.config.production.js --mode production",
    "prepublish": "yarn build",
    "lint": "yarn lint:css && yarn lint:js",
    "lint:css": "stylelint './src/**/*.js'",
    "lint:js": "eslint ./src/**/",
    "deploy": "yarn build:docs && cp CNAME ./docs && gh-pages -d docs",
    "test:cypress": "start-server-and-test start http://localhost:4000 cypress:ci",
    "cypress:ci": "percy exec -- cypress run",
    "cypress": "cypress open"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@percy/cypress": "^0.2.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-inline-react-svg": "^1.0.1",
    "babel-plugin-named-asset-import": "^0.2.3",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cypress": "^3.1.5",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "^7.9.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "rollup": "^1.3.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "start-server-and-test": "^1.7.11",
    "styled-components": "^4.1.3",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "peerDependencies": {
    "react": ">= 16.3.0",
    "react-dom": ">= 16.3.0",
    "styled-components": ">= 4.0"
  },
  "dependencies": {
    "downshift": "^3.2.6",
    "react-modal": "^3.8.1",
    "react-select": "^2.4.1",
    "react-tabs": "^3.0.0",
    "react-tooltip": "^3.9.2",
    "react-transition-group": "^2.6.0",
    "styled-system": "^4.0.0"
  }
}
